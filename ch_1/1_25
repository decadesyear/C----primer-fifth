#include <iostream>
#include "Sales_item.h"

using namespace std;
int main()
{
	Sales_item total;
	if (cin >> total)
	{
		Sales_item trans;
		while (cin >> trans)
		{
			if (total.isbn() = trans.isbn())
			{
				total += trans;
			}
			else
			{
				cout << total << endl;
				total = trans;
			}
		}
		cout << total << endl;//while语句终止时，total中保存着最后一个ISBN数据，需要将这个也打印出来
	}
	else
	{
		cout << "NO DATA?" << endl;
	}
	return 0;
}
